{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "auto",
        "loose": true,
        "shippedProposals": true,
        "targets": {
          "browsers": [">2%"]
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-proposal-private-private-property-in-object", { "loose": true }],
    "@babel/plugin-transform-runtime"
  ]
}
